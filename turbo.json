{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["lib/**"]
      },
      "coverage": {
        "dependsOn": ["build", "^coverage"]
      },
      "test": {
        "dependsOn": ["build", "^test"]
      }
    }
  }
